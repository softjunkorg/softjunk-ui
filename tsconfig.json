{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "jsx": "react-jsx",
        "sourceMap": false,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": true,
        "resolveJsonModule": true
    },
    "include": ["src/*"]
}
