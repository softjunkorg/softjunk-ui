{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "jsx": "react",
        "outDir": "dist",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "allowJs": true,
        "emitDeclarationOnly": true
    },
    "include": ["src/*"]
}
